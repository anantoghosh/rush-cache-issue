/**
 * This is the main configuration file for Rush.
 * For full documentation, please see https://rushjs.io
 */
{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.63.1",
  "pnpmVersion": "6.32.3",
  "pnpmOptions": {
    "strictPeerDependencies": false,
    "resolutionStrategy": "fewer-dependencies",
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=14.15.0",
  "ensureConsistentVersions": true,
  "projectFolderMaxDepth": 3,
  "gitPolicy": {
  },

  "repository": {
    "url": "https://github.com/anantoghosh/rush-cache",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "variants": [
  ],
  "projects": [
    {
      "packageName": "@test/logger",
      "projectFolder": "packages/logger"
    },
    {
      "packageName": "blank",
      "projectFolder": "packages/blank"
    }
  ]
}
